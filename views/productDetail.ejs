					            <% if(listing_type =="FixedPrice"){%>
					            	 <div class="qty"> <strong>Qty:</strong> <a class="qtyBtn mines" href="javascript:void(0);">-</a>
					                  <input id="qty" type="text" class="w30" name="quantity" size="2" value="0" />
					                  <a class="qtyBtn plus" href="javascript:void(0);">+</a>
					                  <input type="hidden" name="product_id" size="2" value="30" />
					                  <div class="clear"></div>
					                </div>
					        		<input type="button" value="Add to Cart" id="button-cart" class="button" />
					        		<input type="button" value="Buy Now" id="buy-now" class="button" />
					        		<script>
					        			$('#button-cart').click(function(){
					        				var qnty = $('#qty').val();
					        				
					        				location.href="/addtoCart?id=<%= product_id%>&price=<%= instant_sellingprice%>&title=<%= product_title%>&qty="+qnty;
					        			});
					        			$('#buy-now').click(function(){
					        				var qnty = $('#qty').val();
					        				location.href="/buynow?id=<%= product_id%>&price=<%= instant_sellingprice%>&title=<%= product_title%>&qty="+qnty;
					        			});
					        		</script>
					        	<% } else { %>
						        		<input type='textbox' id="bid_price" required/>
					        			<input type="button" value="Place Bid" id="place-bid" class="button" />
					        			<script>
						        			$('#place-bid').click(function(){
						        				var price = $('#bid_price').val();
						        				
						        				if(price > <%= instant_sellingprice %> ){ 
						        					location.href="/bid?product_id=<%= product_id%>&product_title=<%= product_title%>&product_description=<%= product_description%>&auction_id=<%= auction_id%>&val="+price;
						        				} else {
						        					$('#bid_price').focus();
						        				}
						        			});
						        		</script>	
					        	<% } %>	
